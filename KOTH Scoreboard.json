{
"Key": "koth_scoreboard",
"Name": "Koth Scoreboard",
"Author": "PresDaGod",
"Quest": false,
"AllowRejoining": true,

"Comment": "loop",
"Add" ["counter", 1],
"SetScoreboard": [
  "&6&lKOTH",
  "[player_high_name] = [koth_player_score]"
],

"If": [],
"Equals": [
  "server",
  "koth_start_value",
  "<",
  "[koth_player_score]"
],
"Skip": "changeScoreboard",
"Skip": "loop",


"Comment": "changeScoreboard",
"SetPersistent": ["server", "player_high_name", "[PLAYER]"],

"Wait": 10,

"If" [],
"Equals": [
  "counter",
  ">",
900
],
"Skip": "LeaveAll",

"Skip": "loop",

"LeaveAll": [],

}
